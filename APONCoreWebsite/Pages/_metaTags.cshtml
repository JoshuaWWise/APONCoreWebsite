
@model APONCoreWebsite.Pages._metaTagsModel

@if (Model.IMTS.getMetaData().description != "")
{
<meta property="og:url" content=@Model.IMTS.getMetaData().URL />
<meta property="og:type" content=@Model.IMTS.getMetaData().type />
<meta property="og:title" content=@Model.IMTS.getMetaData().title />
<meta property="og:description" content=@Model.IMTS.getMetaData().description />
<meta property="og:image" content=@Model.IMTS.getMetaData().image />
<meta name="twitter:card" content="summary" />
<meta name="twitter:site" content="&#64;allportsopen" />
<meta name="twitter:title" content=@Model.IMTS.getMetaData().title />
<meta name="twitter:description" content=@Model.IMTS.getMetaData().description />
<meta name="twitter:image" content=@Model.IMTS.getMetaData().image />
}